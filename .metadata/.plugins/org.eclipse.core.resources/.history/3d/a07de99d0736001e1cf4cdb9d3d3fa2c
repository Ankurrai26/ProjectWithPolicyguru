package BasicJavaPrac;

public class Base {
	static String[] ankur = {"ankur","divya","rups"};
	static int [] count = new int [5];
	
	public static void main(String[] args) {
		
		for (int i=0;i<=ankur.length-1;i++) {
			
			System.out.println(ankur[i]);
		}
		
	count[0] = 1;
	count[1] = 2;
	count[2] = 3;
	count[3] = 4;
	count[4] = 4;
	count[5] = 5;
	
	for (int j=0;j<=count.length-1;j++) {
		
		System.out.println(count[j]);
	}
		
		
		
	}
	
	
}
